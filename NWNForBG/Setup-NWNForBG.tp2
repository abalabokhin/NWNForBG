//Sounds
// LAF HANDLE_AUDIO END

ACTION_IF NOT DIRECTORY_EXISTS ~NWNForBG/Audio/%LANGUAGE%~ BEGIN
	MKDIR ~NWNForBG/Audio/%LANGUAGE%~
  END

ACTION_BASH_FOR ~NWNForBG/Audio/english~ ~^.+\.ogg$~ BEGIN
	ACTION_IF NOT FILE_EXISTS ~NWNForBG/Audio/%LANGUAGE%/%BASH_FOR_FILE%~ BEGIN
		COPY ~%BASH_FOR_FILESPEC%~ ~NWNForBG/Audio/%LANGUAGE%~
  END
END

LAF HANDLE_AUDIO
	STR_VAR
	audio_path = EVAL ~NWNForBG/Audio/%LANGUAGE%~
	oggdec_path = ~NWNForBG/Audio~
END


