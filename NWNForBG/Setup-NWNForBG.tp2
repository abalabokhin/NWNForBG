// Install all animations. It is only valid for ee.
ACTION_IF (GAME_IS ~bg2ee eet~) BEGIN
    // First: install animation that exists in ee animate.ids, but ini file and animation files themselved might not
    // exist in the game.
    ACTION_FOR_EACH animation IN
        ~abishai_red~
        ~animated_plate_bronze~
        ~arundel~
        ~barbarian_shaman_staff~
        ~barbarian_warrior_brown~
        ~barbarian_warrior_tan~
        ~bugbear~
        ~bugbear_captain~
        ~creeping_doom~
        ~elemental_water~
        ~feyr~
        ~feyr_greater~
        ~ghost~
        ~giant_fire_2~
        ~giant_fire_iwd~
        ~goblin_captain~
        ~goblin_shaman~
        ~golem_iron_iwd~
        ~harpy~
        ~histachii~
        ~hook_horror~
        ~isair~
        ~lagoon_creature~
        ~lich_white~
        ~lizard_king~
        ~lizard_man~
        ~madae~
        ~malarite~
        ~orog_shaman~
        ~shadow_small~
        ~tundra_yeti~
        ~wererat~
        ~wight_barrow~
        ~worg~
        ~zombie_blue~
        ~zombie_yellow~
    BEGIN
        ACTION_BASH_FOR ~%MOD_FOLDER%/missing_anim_ee/%animation%~ ~.+$~ BEGIN
            // We copy only the files we do need.
            ACTION_IF NOT FILE_EXISTS_IN_GAME ~%BASH_FOR_FILE%~ BEGIN
                COPY_LARGE ~%BASH_FOR_FILESPEC%~ ~override/%BASH_FOR_FILE%~
            END
        END
    END

    ACTION_DEFINE_ASSOCIATIVE_ARRAY anim_to_min_slot BEGIN
        ~amel_pc_nwn~ => ~0xe000~
        ~bebilith_nwn~ => ~0xe000~
        ~succubus_nwn~ => ~0xe000~
        ~displacer_beast~ => ~0x7000~
        ~giant_fire_female~ => ~0x7000~
        ~goblinelite_axe_green~ => ~0xe000~
        ~goblinelite_bow_green~ => ~0xe000~
        ~dustman_female_pst~ => ~0x7000~
        ~dustman_male_pst~ => ~0x7000~
        ~godsman_pst~ => ~0x7000~
        ~harlot_pst~ => ~0x7000~
        ~merchant_pst~ => ~0x7000~
        ~moinesse_ninja_male_human~ => ~0x5000~
        ~skeleton_priest_pst~ => ~0x7000~
        ~skeleton_worker_pst~ => ~0x7000~
        ~thug_pst~ => ~0x7000~
        ~townie_curst_male_pst~ => ~0x7000~
        ~townie_lower_class_female_pst~ => ~0x7000~
        ~townie_lower_class_male_pst~ => ~0x7000~
        ~townie_upper_class_female_pst~ => ~0x7000~
        ~townie_upper_class_male_pst~ => ~0x7000~
        ~townie_wizard_pst~ => ~0x7000~
    END

    ACTION_PHP_EACH anim_to_min_slot AS animation => min_slot
    BEGIN
        OUTER_TEXT_SPRINT upp_anim ~%animation%~
        ACTION_TO_UPPER upp_anim
        OUTER_SET slot = IDS_OF_SYMBOL (~animate~ ~%upp_anim%~)
        ACTION_IF slot<0 BEGIN
            LAF ~FIND_FREE_ANIM_SLOT~ INT_VAR slotMin = min_slot STR_VAR GWFormatAnim = BG2SPLIT RET slot END
            ACTION_IF (slot < 0) BEGIN
                WARN @1141  // ~!!! WARNING : No free creature animation slot found for %GWNewAnim% !!!~
            END ELSE BEGIN
                LAF ~TO_HEX_NUMBER~ INT_VAR value = slot minDigits = 4 RET hexNumber END
                APPEND ~animate.ids~ ~0x%hexNumber% %upp_anim%~
                CLEAR_IDS_MAP

                ACTION_BASH_FOR ~%MOD_FOLDER%/ia_cre/%animation%~ ~.+$~ BEGIN
                    // We copy only the files we do need.
                    ACTION_IF NOT FILE_EXISTS_IN_GAME ~%BASH_FOR_FILE%~ BEGIN
                        ACTION_TO_LOWER %BASH_FOR_FILE%
                        COPY_LARGE ~%BASH_FOR_FILESPEC%~ ~override/%BASH_FOR_FILE%~
                    END
                END
                COPY ~%MOD_FOLDER%/ia_ini/%animation%.ini~ ~override/%hexNumber%.ini~
            END
        END
    END
END

// Add Local Variable ("NWBURNHA" = 1) on every target of burning hands (for NWBELIAL Summoning quest).
COPY_EXISTING ~SPWI103.SPL~ ~override/SPWI103.SPL~
  READ_LONG 0x64 abil_off
  READ_SHORT 0x68 abil_num
  READ_LONG 0x6a fx_off

  FOR (i = 0; i < "%abil_num%"; i += 1) BEGIN //parse each ability
  //verify specific ability - check the ability is the correct one to change here, must be unique
  READ_BYTE (%abil_off% + %i% * 0x28) abil_type
  PATCH_IF (%abil_type% = 1) BEGIN
    READ_SHORT (%abil_off% + %i% * 0x28 + 0x1e) abil_fx_num
    READ_SHORT (%abil_off% + %i% * 0x28 + 0x20) abil_fx_idx

    //add effect
    INSERT_BYTES (%fx_off% + %abil_fx_idx% * 0x30) 0x30

    //effect fields
    WRITE_SHORT (%fx_off% + %abil_fx_idx% * 0x30) 309 //type
    WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0x2) 2 //target
    WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0x3) 1 //power
    WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x4) 1 //param1
    WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x8) 0 //param2
    WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0xc) 10 //timing mode
    WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0xd) 0 //dispel/resistance
    WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0xe) 5 //duration
    WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0x12) 100 //prob1
    WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0x13) 0 //prob2
    WRITE_ASCII (%fx_off% + %abil_fx_idx% * 0x30 + 0x14) ~NWBURNHA~ #8 //resource
    WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x1c) 0 //#dice, maxlevel
    WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x20) 0 //dicesize, minlevel
    WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x24) 0 //save type
    WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x28) 0 //save bonus
    WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x2c) 0 //unknown  END

    //update #ability effects
    SET %abil_fx_num% += 1
    WRITE_SHORT (%abil_off% + %i% * 0x28 + 0x1e) %abil_fx_num%

    //update ability effect indices for abilities after current one
    FOR (j = %i% + 1; j < "%abil_num%"; j += 1) BEGIN
      READ_SHORT (%abil_off% + %j% * 0x28 + 0x20) abil_fx_idx
      WRITE_SHORT (%abil_off% + %j% * 0x28 + 0x20) (%abil_fx_idx% + 1)
    END
  END
END

// For oBG add proficiency 117 (PROTECTION_FROM_EVIL) to targets of all variants of this spell (for NWBELIAL Summoning quest).
ACTION_IF GAME_IS ~bg2 tob bgt~ THEN BEGIN
  ACTION_FOR_EACH spell_res IN
    ~SPCL213.SPL~ //Protection From Evil
    ~SPCL233.SPL~ //Protection From Evil 10' radius
    ~SPIN121.SPL~ //Protection From Evil
    ~SPPR107.SPL~ //Protection From Evil
    ~SPPR408.SPL~ //Protection From Evil 10' radius
    ~SPWI113.SPL~ //Protection From Evil
  BEGIN
    COPY_EXISTING ~%spell_res%~ ~override/%spell_res%~
      READ_LONG 0x64 abil_off
      READ_SHORT 0x68 abil_num
      READ_LONG 0x6a fx_off

      FOR (i = 0; i < "%abil_num%"; i += 1) BEGIN //parse each ability
      //verify specific ability - check the ability is the correct one to change here, must be unique
      READ_BYTE (%abil_off% + %i% * 0x28) abil_type
      PATCH_IF (%abil_type% = 1) BEGIN
        READ_SHORT (%abil_off% + %i% * 0x28 + 0x1e) abil_fx_num
        READ_SHORT (%abil_off% + %i% * 0x28 + 0x20) abil_fx_idx

        effect_duration = 180
        //Read the duration of Display Portrait Icon effect if any
        FOR (j = "%abil_fx_idx%"; j < "%abil_fx_idx%" + "%abil_fx_num%"; j += 1) BEGIN
          READ_SHORT (%fx_off% + %j% * 0x30) effect_type
          PATCH_IF (%effect_type% = 142) BEGIN
            READ_LONG (%fx_off% + %j% * 0x30 + 0xe) effect_duration
          END
        END

        //add effect
        INSERT_BYTES (%fx_off% + %abil_fx_idx% * 0x30) 0x30

        //effect fields
        WRITE_SHORT (%fx_off% + %abil_fx_idx% * 0x30) 233 //type
        WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0x2) 2 //target
        WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0x3) 1 //power
        WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x4) 1 //param1
        WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x8) 117 //param2
        WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0xc) 0 //timing mode
        WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0xd) 3 //dispel/resistance
        WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0xe) %effect_duration% //duration
        WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0x12) 100 //prob1
        WRITE_BYTE (%fx_off% + %abil_fx_idx% * 0x30 + 0x13) 0 //prob2
        WRITE_ASCII (%fx_off% + %abil_fx_idx% * 0x30 + 0x14) ~~ #8 //resource
        WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x1c) 0 //#dice, maxlevel
        WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x20) 0 //dicesize, minlevel
        WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x24) 0 //save type
        WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x28) 0 //save bonus
        WRITE_LONG (%fx_off% + %abil_fx_idx% * 0x30 + 0x2c) 0 //unknown  END

        //update #ability effects
        SET %abil_fx_num% += 1
        WRITE_SHORT (%abil_off% + %i% * 0x28 + 0x1e) %abil_fx_num%

        //update ability effect indices for abilities after current one
        FOR (j = %i% + 1; j < "%abil_num%"; j += 1) BEGIN
          READ_SHORT (%abil_off% + %j% * 0x28 + 0x20) abil_fx_idx
          WRITE_SHORT (%abil_off% + %j% * 0x28 + 0x20) (%abil_fx_idx% + 1)
        END
      END
    END
  END
END

