//Spells
COPY ~NWNForBG/spl~ ~override~
INCLUDE ~NWNForBG/lib/names_for_spells.tph~

//Add new wizard spells
PRINT ~Adding new wizard spells~

OUTER_SPRINT current_spell_name ~WIZARD_SNILLOCS_SNOWBALL_SWARM~
OUTER_SPRINT current_spell_scroll_res ~NWSCSNIL.ITM~
OUTER_SET spell_found = 0
OUTER_SET spell_slot = IDS_OF_SYMBOL (~spell~ ~%current_spell_name%~)
ACTION_IF spell_slot>=0 THEN BEGIN
  LAF RES_NUM_OF_SPELL_NAME
    STR_VAR
      spell_name=EVAL ~%current_spell_name%~
    RET
      current_spell_res=spell_res
  END
  ACTION_IF FILE_EXISTS_IN_GAME ~%current_spell_res%.SPL~ THEN BEGIN
    OUTER_SET spell_found = 1
    PRINT ~%current_spell_name% is found, it will be used in NWNForBG~
    COPY_EXISTING ~%current_spell_res%.SPL~ ~override~
      READ_LONG 0x8 spell_name
      READ_LONG 0x50 spell_desc
    BUT_ONLY_IF_IT_CHANGES
    COPY_EXISTING ~%current_spell_scroll_res%~ ~override~
      WRITE_LONG 0xC spell_name
      WRITE_LONG 0x54 spell_desc
    BUT_ONLY_IF_IT_CHANGES
  END
END

ACTION_IF NOT spell_found THEN BEGIN
  PRINT ~%current_spell_name% is not found, adding it~
  ADD_SPELL ~%MOD_FOLDER%/new_wi_spl/SPWI204.SPL~ 2 2 ~WIZARD_SNILLOCS_SNOWBALL_SWARM~
    SAY 0x8      @103221
    SAY 0x50     @103222
    REPLACE_TEXTUALLY CASE_INSENSITIVE EXACT_MATCH ~SPWI204~ ~%DEST_RES%~
    SPRINT current_spell_res ~%DEST_RES%~
END

COPY_EXISTING ~%current_spell_scroll_res%~ ~override~
  WRITE_ASCIIE 0xf6  ~%current_spell_res%~ #7
  WRITE_ASCIIE 0x126 ~%current_spell_res%~ #7

