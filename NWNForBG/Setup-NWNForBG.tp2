ACTION_IF (GAME_IS ~bg2ee eet~) BEGIN
  INCLUDE ~NWNForBG/lib/a7_pvrz_copying.tpa~
  // Installing all TIS files from the specified folder
  ACTION_BASH_FOR ~%MOD_FOLDER%/map_ee~ ~^.+\.tis$~ BEGIN
    COPY ~%BASH_FOR_FILESPEC%~ ~override~
      LPF UPDATE_PVRZ_INDICES RET original_base_index new_base_index END

    // Installing associated PVRZ files for each tileset individually
    ACTION_IF (new_base_index >= 0) BEGIN
      LAF TIS_RES_TO_PVRZ STR_VAR tis_res = EVAL ~%BASH_FOR_FILESPEC%~ RET prefix END

      ACTION_BASH_FOR ~%MOD_FOLDER%/map_ee~ ~^%prefix%.+\.pvrz$~ BEGIN
        LAF INSTALL_PVRZ
          INT_VAR original_base_index new_base_index
          STR_VAR source_file = EVAL ~%BASH_FOR_FILESPEC%~
          RET success DEST_FILESPEC
        END

        ACTION_IF (NOT success) BEGIN
          FAIL ~Could not install "%BASH_FOR_FILESPEC%"!~
        END
      END
    END ELSE BEGIN
      FAIL ~Could not install "%BASH_FOR_FILESPEC%"!~
    END
  END
END ELSE BEGIN
  INCLUDE ~NWNForBG/lib/a7_tools.tpa~
  // installing TIS for vanila
  ACTION_BASH_FOR  ~NWNForBG/map_ee~ ~.*\.TIS$~ BEGIN
    LAF HANDLE_TILE2EE
      INT_VAR
        silent = 0  // show feedback
      STR_VAR
        input_path = EVAL ~%BASH_FOR_FILESPEC%~
      RET
        num_converted
    END
    ACTION_IF (num_converted != 1) BEGIN
      FAIL ~Error converting %file%~
    END
  END
  // installing MOS, some TIS and WED files (the ones with water overlay that cannot be 
  // properly converted with HANDLE_TILE2EE
  COPY ~NWNForBG/map~ ~override~
END

ACTION_IF (GAME_IS ~bg2 tob bgt bg2ee eet~ AND FILE_EXISTS_IN_GAME ~BALDUR.BCS~) BEGIN

  // Prevent specific events from triggering during NWN travels (Borrowed from TotLM by Argent77 )
  ACTION_DEFINE_ASSOCIATIVE_ARRAY area_checks BEGIN
    ~baldur~    => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~aerie~     => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~anomen~    => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~cernd~     => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~edwin~     => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~jaheira~   => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~jan~       => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~keldorn~   => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~korgan~    => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~mazzy~     => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~minsc~     => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~nalia~     => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
    ~yoshimo~   => ~\(!Global("Chapter","GLOBAL",%bg2_chapter_4%)\)~
  END

  OUTER_TEXT_SPRINT bcs_quest_check ~!Global("NWN_Quest_Status","GLOBAL",1)~
  ACTION_PHP_EACH area_checks AS script => search BEGIN
    COPY_EXISTING ~%script%.bcs~ ~override~
      DECOMPILE_AND_PATCH BEGIN
        REPLACE_TEXTUALLY ~%search%~ ~%bcs_quest_check% \1~
      END
    BUT_ONLY IF_EXISTS
  END
END

