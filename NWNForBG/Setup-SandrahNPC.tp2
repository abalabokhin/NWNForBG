  PATCH_IF SOURCE_SIZE > 0x11b BEGIN
    READ_ASCII 0x94 ~rsc~ //Area script
    PATCH_IF (~%rsc%~ STRING_EQUAL_CASE ~none~ = 0) AND (~%rsc%~ STRING_EQUAL ~~ = 0) BEGIN //If not a null script
      PATCH_IF NOT VARIABLE_IS_SET $df(EVALUATE_BUFFER ~%rsc%~) BEGIN
        SPRINT $df(EVALUATE_BUFFER ~%rsc%~) ~%rsc%~
      END
    END ELSE BEGIN
      SPRINT ~rsc~ ~%SOURCE_RES%~
      PATCH_IF FILE_EXISTS_IN_GAME ~%rsc%.bcs~ BEGIN
        WRITE_ASCIIE 0x94 ~%rsc%~ #8 
        SPRINT $df(EVALUATE_BUFFER ~%rsc%~) ~%rsc%~
      END ELSE BEGIN
        INNER_ACTION BEGIN
          COPY_EXISTING ~pellfoll.bcs~ ~override/%rsc%.bcs~
        END
        WRITE_ASCIIE 0x94 ~%rsc%~ #8
      END
    END
  END
BUT_ONLY_IF_IT_CHANGES

